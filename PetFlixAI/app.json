{
  "expo": {
    "name": "PetFlixAI",
    "slug": "PetFlixAI",
    "scheme": "petflixai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juliajacobs.PetFlixAI",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "PetFlix AI needs access to your camera to take photos of your pet for their movie.",
        "NSPhotoLibraryUsageDescription": "PetFlix AI needs access to your photo library to select photos of your pet for their movie.",
        "NSPhotoLibraryAddUsageDescription": "PetFlix AI needs access to save the generated movie to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juliajacobs.PetFlixAI"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f4e32f70-9c23-4c92-9f2a-bea82796818b"
      }
    }
  }
}
